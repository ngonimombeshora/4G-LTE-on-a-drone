<scenario name="Rel10_s1ap_ue_ctxt_rel_after_identity_request">

  <!-- VARIABLES TO MME -->


  <!-- VARIABLES FROM UE -->


  <!-- VARIABLES COMPUTED BY MME SCENARIO PLAYER -->
